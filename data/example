negate(existentialQuantifier(country, firstPersonAssociate(quantifiedAlias)))
exclusiveOr(world, nowhere)
isEqual(firstPersonAssociate(country), previousStatement)

negate(existentialQuantifier(family, firstPersonAssociate(quantifiedAlias)))
exclusiveOr(mankind, nobody)
isEqual(firstPersonAssociate(family), previousStatement)

imperative(destroy(firstPerson, nextStatement))
and(voidAll(hate), voidAll(love))